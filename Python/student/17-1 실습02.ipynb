    "def calc(operation, *args):\n",
    "    if operation == '+':\n",
    "        return sum(args)\n",
    "    elif operation == '*':\n",
    "        result = 1\n",
    "        for arg in args:\n",
    "            result *= arg\n",
    "        return result\n",
    "\n",
    "print(calc(\"+\", 1, 2, 3, 4, 5))\n",
    "print(calc(\"*\", 1, 2, 3, 4, 5))\n"
   ]
  }
