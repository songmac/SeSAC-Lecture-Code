   ],
   "source": [
    "i = -1\n",
    "sum = 0\n",
    "\n",
    "while i != 0:\n",
    "    i = int(input(\"값을 입력해주세요 : \"))\n",
    "    sum += i\n",
    "\n",
    "print(f\"합계는? {sum}\")"
   ]
