   "source": [
    "def allindex(lis, x):\n",
    "\n",
    "    idx_list = []\n",
    "    for idx, val in enumerate(lis):\n",
    "        if val == x:\n",
    "            idx_list.append(idx)\n",
    "    \n",
    "    return idx_list\n",
    "\n",
    "lis = [1, 2, 3, 1, 4, 2, 1]\n",
    "allindex(lis, 1)"
   ]
  }
